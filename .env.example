# Copy this file to `.env` and fill in your secrets.
# Keep `.env` out of version control.

# Server auth (JWT)
# Algorithm defaults to HS256; set secret for signed tokens
JWT_ALG=HS256
JWT_SECRET=

# Server cache limits (optional overrides)
# FEED_CACHE_MAX_ENTRIES=1000
# RATE_BUCKET_MAX_ENTRIES=5000
# DASH_CACHE_MAX_ENTRIES=500
# TREND_CACHE_MAX_ENTRIES=500

# RAG client cache limits (optional overrides)
# RAG_CACHE_MAX_ENTRIES=5000
# RAG_CACHE_TTL_SECONDS=86400

# Telegram (Telethon)
TELEGRAM_API_ID=
TELEGRAM_API_HASH=

# X (Twitter) API v2
TWITTER_BEARER_TOKEN=

# YouTube Data API v3 (optional; RSS fallback used if not set)
YOUTUBE_API_KEY=